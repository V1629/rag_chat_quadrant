# Environment variables - copy to .env and fill in your values
GEMINI_API_KEY=your_gemini_api_key_here
EMBEDDING_MODEL=all-MiniLM-L6-v2
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/rag_db

# Qdrant Configuration
# For local Qdrant: use http://localhost:6333 and leave QDRANT_API_KEY empty
# For Qdrant Cloud: use your cluster URL and provide API key
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key_here_if_using_cloud
